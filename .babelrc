{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "12"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "./utils": "utils",
          "./cleaners": "cleaners",
          "./resource": "resource",
          "./extractors": "extractors",
          "./mercury.js": "mercury"
        }
      }
    ]
  ]
}
